{
  "name": "@retorquere/zotero-sync",
  "version": "1.0.6",
  "description": "One-way sync of Zotero libraries",
  "scripts": {
    "tsc": "tsc",
    "prepublishOnly": "pnpm install && npm run tsc",
    "go": "npm run tsc && npm version patch && npm publish --access public",
    "ncu": "ncu -u && pnpm i && git add package.json pnpm-lock.yaml && git commit -m ncu",
    "postversion": "git push --follow-tags"
  },
  "main": "index.js",
  "typings": "./typings/global.d.ts",
  "author": "Emiliano Heyns",
  "license": "AGPL",
  "devDependencies": {
    "@types/node": "^14.14.41",
    "ejs": "^3.1.6",
    "eslint": "^7.24.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "json-stringify-pretty-compact": "^3.0.0",
    "node-fetch": "^2.6.1"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "index.ts",
    "json-store.js",
    "package.json",
    "pnpm-lock.yaml",
    "tsconfig.json",
    "typings/global.d.ts"
  ]
}
